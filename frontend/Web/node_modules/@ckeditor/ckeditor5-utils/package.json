{
  "name": "@ckeditor/ckeditor5-utils",
  "version": "12.1.1",
  "description": "Miscellaneous utils used by CKEditor 5.",
  "keywords": [
    "ckeditor",
    "ckeditor5",
    "ckeditor 5",
    "ckeditor5-lib"
  ],
  "dependencies": {
    "ckeditor5": "^12.2.0",
    "lodash-es": "^4.17.10"
  },
  "devDependencies": {
    "@ckeditor/ckeditor5-build-classic": "^12.2.0",
    "@ckeditor/ckeditor5-core": "^12.1.1",
    "@ckeditor/ckeditor5-engine": "^13.1.1",
    "eslint": "^5.5.0",
    "eslint-config-ckeditor5": "^1.0.11",
    "husky": "^1.3.1",
    "lint-staged": "^7.0.0"
  },
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.7.1"
  },
  "author": "CKSource (http://cksource.com/)",
  "license": "GPL-2.0-or-later",
  "homepage": "https://ckeditor.com/ckeditor-5",
  "bugs": "https://github.com/ckeditor/ckeditor5-utils/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/ckeditor/ckeditor5-utils.git"
  },
  "files": [
    "lang",
    "src",
    "theme"
  ],
  "scripts": {
    "lint": "eslint --quiet '**/*.js'"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --quiet"
    ]
  },
  "eslintIgnore": [
    "src/lib/**",
    "packages/**"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
